{
  "manifest_version": 3,
  "name": "IKI SCAN WEB AI",
  "version": "1.1",
  "description": "Mendeteksi web buatan AI dan kebocoran API Key Supabase",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}